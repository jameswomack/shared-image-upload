{
  "name": "shared-image-upload",
  "version": "1.0.0",
  "description": "Upload an image from one device and view it on another in \"real time\"",
  "main": "index.js",
  "scripts": {
    "build": "babel ./static/esnext -d ./static/js",
    "watch": "babel --watch ./static/esnext -d ./static/js",
    "start": "node index.js & npm run watch"
  },
  "author": "James J. Womack <james@womack.io>",
  "repository": "https://github.com/jameswomack/shared-image-upload.git",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.4.5",
    "express": "4.13.4",
    "socket.io": "1.4.5",
    "socketio-file-upload": "0.4.4"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-stage-0": "6.5.0"
  }
}
